<%- contentFor('content') %>


<div class="panel panel-default col-md-6 col-md-offset-3">
    <div class="panel-heading">ChitChat</div>
    <div class="panel-body">
        <ul class="list-group" id="arena">
            <li class="list-group-item">
                Greetings
            </li>
            <li class="list-group-item">
                Salam
            </li>
            <li class="list-group-item">
                Bye
            </li>
            <li class="list-group-item">
                <form id="form">

                    <div class="form-group">
                        <div class="input-group">
                            <input id="msg" type="text" class="form-control" name="msg" placeholder="Type your message">
                            <span class="input-group-btn">
                                  <button class="btn btn-default" type="submit">Enter</button>
                            </span>
                        </div>
                    </div>
                </form>
            </li>
        </ul>
    </div>
</div>